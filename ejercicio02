
.data
	STR2:  .asciiz "    Hello World"
	STR1:	.asciiz "Hello World"

.text
main:
  	la $a0 STR1
  	jal strlen_1				#llamar subrutina de longitud
  	
  
  	la $a0 STR2
  	jal skipspaces_1			#llamar a la subrutina
  



strlen_1:    move $a0 $t0
	li $v0 0				#contador
	li $t3 1

bcle:   lbu $t1 ($t0) 				#char
	beq $t0 $t1 fin				#si fin de string, salto a fin
	add $v0 $v0 $t3				#aumentar contador
	add $t0 $t0 $t3				#aumentar direccion de memoria
	b bcle

fin:	jr $ra

skipspaces_1: 	move $v0 $a0
	li $t0 32				#space char	
	li $t3 1

bTrm: 	lbu $t1 ($v0)				#char
	bne $t0 $t1 finT
	add $v0 $v0 $t3				#aumentar direccion de memoria
	b bTrm

finT: 	jr $ra
